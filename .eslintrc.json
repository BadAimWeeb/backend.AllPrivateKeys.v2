{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-async-promise-executor": 0,
        "no-empty": [0, { "allowEmptyCatch": true }]
    }
}
